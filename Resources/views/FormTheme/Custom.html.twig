{% block feature_settings_row %}
<script>
    fetch('/menuitem')
        .then(response => {
            if (!response.ok) {
                throw new Error('Network response was not ok');
            }
            return response.text(); // Get HTML as text
        })
        .then(html => {
            console.log('Fetched HTML:', html);

            // Optional: parse the HTML into a DOM object
            const parser = new DOMParser();
            const doc = parser.parseFromString(html, 'text/html');

            // Example: extract all menu item links
            const menuItems = doc.querySelectorAll('a'); // adjust selector based on HTML structure
            menuItems.forEach(item => {
                console.log(item.textContent, item.href);
            });
        })
        .catch(error => {
            console.error('There was a problem with the fetch operation:', error);
        });
</script>
